{
  "manifest_version": 2,
  "name": "Quasify",
  "version": "0.0.0.2",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
         "jquery-2.1.4.min.js",
         "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "quaz 2.png"
  }
}
